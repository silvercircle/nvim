{
  "emdash": {
    "prefix": "emdash",
    "body": "—"
  },
  "endash": {
    "prefix": "endash",
    "body": "–"
  },
  "dquotes": {
    "prefix": "dquotes",
    "body": "„$0“"
  },
  "iquotes": {
    "prefix": "iquotes",
    "body": "*„$1“*$2"
  },
  "bracks": {
    "prefix": "bracks",
    "body": "‹$0›"
  },
  "dbracks": {
    "prefix": "dbracks",
    "body": "«$0»"
  },
  "frontmatter": {
    "prefix": "frontmatter",
    "body": [
      "---",
      "author: $0",
      "title: $1",
      "date: $2",
      "modified: $3",
      "layout: no_sidebar",
      "categories: []",
      "menucontext: topics",
      "section: $4",
      "backlink: /blog",
      "isso: true",
      "words: ",
      "reduced: small",
      "tag: []",
      "keywords: []",
      "excerpt: >",
      "  $5",
      "image:",
      "  source: ",
      "  width: 100px",
      "  align: right",
      "share: 1",
      "---"
    ]
  }
}
