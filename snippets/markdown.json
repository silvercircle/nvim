{
  "zkfrontmatter": {
    "prefix": "zkfrontmatter",
    "body": [
      "---",
      "title: $1",
      "tags: $2",
      "date: $3",
      "---",
      ""
    ]
  },
  "imageleft": {
    "prefix": "imageleft",
    "body": [
      "![$1]($2){align=left width=$3px}"
    ]
  },
  "imageright": {
    "prefix": "imageright",
    "body": [
      "![$1]($2){align=right width=$3px}"
    ]
  },
  "running": {
    "prefix": "running",
    "body": [ "**Running time:** $1 minutes ($2 hr, $3 min), **Seen on:** $4."]
  }
}
