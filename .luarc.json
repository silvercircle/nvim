{
    "diagnostics": {
        "workspaceEvent": "None",
        "workspaceRate": 100,
        "workspaceDelay": 10000,
        "groupFileStatus": {
            "ambiguity": "Any",
            "await": "Any",
            "duplicate": "Any",
            "global": "Any",
            "luadoc": "Any",
            "redefined": "Any",
            "strict": "Any",
            "type-check": "Any",
            "unbalanced": "Any",
            "unused": "Any"
        },
        "disable": [
          "param-type-mismatch",
          "undefined-field",
          "invisible",
          "missing-global-doc",
          "incomplete-signature-doc",
          "inject-field"
        ]
    },
    "runtime": {
        "version": "LuaJIT"
    },
    "workspace": {
        "maxPreload": 1600,
        "preloadFileSize": 1000,
        "checkThirdParty": "Disable"
    },
    "typeFormat": {
        "config": {
            "format_line": "false"
        }
    },
    "type": {
        "castNumberToInteger": false
    },
    "doc": {
        "privateName": [
            "_*"
        ]
    },
    "window": {
      "progressBar": true
    },
    "format": {
      "enable": true,
      "defaultConfig": {
        "indent_style": "space",
        "indent_size": "2",
        "quote_style": "double"
      }
    }
}
